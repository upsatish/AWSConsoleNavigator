{
  "name": "AWS Console Navigator",
  "description": "Navigate to resources in the AWS console from the Omnibox.",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "omnibox": {
    "keyword": "awss"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Click to set AWS region"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "140": "img/Region_dark-bg@4x.png"
  }
}