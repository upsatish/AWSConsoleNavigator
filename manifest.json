{
  "name": "AWS Console Navigator",
  "description": "Go to AWS console resources from your browser's search box.",
  "version": "2.0.8",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "omnibox": {
    "keyword": "awss"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Click to set AWS region"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "140": "img/Region_dark-bg@4x.png"
  }
}
